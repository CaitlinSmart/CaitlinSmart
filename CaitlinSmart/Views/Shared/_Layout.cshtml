<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/bootstrap.min.css")
    <!-- Custom styles for this template -->
    @Styles.Render("~/css/main.css")
    @Styles.Render("~/css/icomoon.css")
    @Styles.Render("~/css/animate-custom.css")
    @Styles.Render("http://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic")
    @Styles.Render("http://fonts.googleapis.com/css?family=Raleway:400,300,700")

    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
</head>
<body>
    @RenderBody()
    @Scripts.Render("~/Scripts/bootstrap.min.js")
    @Scripts.Render("~/js/modernizr.custom.js")
    @RenderSection("scripts", required: false)
</body>
</html>
